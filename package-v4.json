{
  "name": "create-webbx-app",
  "version": "1.5.1",
  "private": false,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:clean": "rm -rf .next && next build",
    "build:production": "NODE_ENV=production npm run build:clean",
    "start": "next start",
    "type-check": "tsc",
    "type-check:watch": "tsc --watch",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "lint:format": "bun run lint:fix && bun run format"
  },
  "description": "A modern Next.js 15 template with TypeScript, Tailwind CSS, and more",
  "keywords": [
    "nextjs",
    "typescript",
    "tailwind",
    "template",
    "starter"
  ],
  "browserslist": "defaults, not ie <= 11",
  "author": "Thompson Manda <thompsonmanda08@gmail.com>",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thompsonmanda08/next-app-template.git"
  },
  "bugs": {
    "url": "https://github.com/thompsonmanda08/next-app-template/issues"
  },
  "homepage": "https://github.com/thompsonmanda08/next-app-template#readme",
  "bin": {
    "create-webbx-app": "./bin/create-template.js"
  },
  "dependencies": {
    "@heroui/react": "^2.8.2",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.0.0",
